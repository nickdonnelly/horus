   <string notr="true">#scrollAreaContents, #lblAbout{
	background: white;
	border: none;
	padding: 0;
}

#stackedWidget { border-left: 1px solid #777777; }

*{ background: white; }
QRadioButton::indicator { width: 0; height: 0; }

QFrame QLabel {
	background: #FAFAFA;
	border-bottom: 1px solid #777777;
}

QRadioButton {
	background: white;
	margin: 0px;
	padding: 0 0 0 10;
	height: 60px;
	width: 100%;
	icon-size: 0px;
}

QRadioButton:hover {
	background: rgb(239, 239, 239)
}

QRadioButton:checked {
	background: rgba( 3, 155, 229, 255) !important;
}

QRadioButton:checked:hover {
	background: rgb(100, 206, 255) !important;
}

QPushButton {
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgba(3, 155, 229, 255), stop:1 rgba(3, 155, 229, 255));
	color: #000000;
	border-radius: 18px;
}

QPushButton:hover {
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgba(0, 109, 179, 255), stop:1 rgba(0, 109, 179, 255));
	color: #ffffff;
}

QLineEdit {
	background: rgb(3,155,229);
	border-radius: 18px;
	border: 1px solid black;
	color: #000000;
	margin: 0;
	padding: 5px;
}
QLineEdit:focus, QLineEdit:hover {
	background: rgb(0,109,179);
	color: #ffffff;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
	background: none;
	border: none;
	width: 0;
	height: 0;
}

QScrollBar:vertical{
	max-width: 8;
}

QScrollBar::handle:vertical {
	background: rgb(0,109,179);
	border-radius: 4px;
}

#btnDeleteAllLocal {
	background: rgb(243, 14, 14);
	color: #ffffff;
}
#btnDeleteAllLocal:hover {
	background: rgb(255, 0, 0);
}</string>
